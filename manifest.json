{
  "manifest_version": 3,
  "name": "Speed Youtube",
  "description": "Change Youtube Speed",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_title": "Speed Youtube"
  },
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/", "https://www.youtube.com/*"],
      "css": ["css/style.css"],
      "js": ["js/custom.js"]
    }
  ]
}
